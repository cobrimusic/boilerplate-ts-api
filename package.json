{
  "name": "moment-api",
  "version": "1.0.0",
  "description": "Project for learning multiplatform technology",
  "main": "server/server.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/diegodulval/moment-api.git"
  },
  "bugs": "https://github.com/diegodulval/moment-api/issues",
  "scripts": {
    "lint": "./node_modules/.bin/tslint server/*.ts -t verbose",
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "builded": "cross-env NODE_ENV=production node ./dist/server/server.js",
    "unit-test": "cross-env NODE_ENV=test mocha --opts mocha.opts ./dist/tests/unit/unit.test.js",
    "integration-test": "cross-env NODE_ENV=test mocha --opts mocha.opts ./dist/tests/integration/integration.test.js",
    "integration-coverage": "nyc --reporter=html npm run integration-test",
    "unit-coverage": "nyc --reporter=html npm run unit-test",
    "prepush": "npm run lint"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/bluebird": "^3.5.8",
    "@types/body-parser": "^1.16.4",
    "@types/chai": "^4.0.2",
    "@types/express": "^4.0.36",
    "@types/http-status": "^0.2.29",
    "@types/jwt-simple": "^0.5.33",
    "@types/lodash": "^4.14.74",
    "@types/mocha": "^2.2.41",
    "@types/morgan": "^1.7.32",
    "@types/node": "^6.0.88",
    "@types/passport": "^0.3.4",
    "@types/passport-jwt": "^2.0.23",
    "@types/sequelize": "^4.0.68",
    "@types/supertest": "^2.0.2",
    "chai": "^4.1.1",
    "cross-env": "^5.1.4",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-typescript": "^3.2.1",
    "http": "0.0.0",
    "http-server": "^0.9.0",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "nyc": "^10.3.2",
    "supertest": "^3.0.0",
    "testdouble": "^3.2.4",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "express": "^4.15.4",
    "http": "0.0.0",
    "http-status": "^1.0.1",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "pg": "^7.1.0",
    "sequelize": "^4.4.5",
    "sequelize-cli": "^2.8.0"
  },
